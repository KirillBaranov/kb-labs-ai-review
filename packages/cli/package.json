{
  "name": "@sentinel/cli",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "bin": {
    "sentinel": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "ts-node src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@sentinel/core": "workspace:*",
    "@sentinel/provider-mock": "workspace:*",
    "@sentinel/provider-types": "workspace:*",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "license": "MIT",
  "author": {
    "name": "Kirill Baranov",
    "url": "https://github.com/KirillBaranov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirill-baranov/sentinel-ai.git"
  },
  "bugs": {
    "url": "https://github.com/kirill-baranov/sentinel-ai/issues"
  },
  "homepage": "https://github.com/kirill-baranov/sentinel-ai#readme",
  "keywords": [
    "ai",
    "code-review",
    "cli",
    "static-analysis",
    "sentinel"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
