{
  "name": "@kb-labs/ai-review-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "ai-review": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "kb": {
    "commandsManifest": "./dist/cli.manifest.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@kb-labs/ai-review-core": "link:../core",
    "@kb-labs/ai-review-provider-types": "workspace:*",
    "@kb-labs/ai-review-provider-mock": "workspace:*",
    "@kb-labs/ai-review-provider-local": "workspace:*",
    "@kb-labs/ai-review-analytics": "link:../analytics",
    "@kb-labs/core-sys": "link:../../../kb-labs-core/packages/sys",
    "@kb-labs/shared-review-types": "link:../../../kb-labs-shared/packages/review-types",
    "@kb-labs/cli-core": "link:../../../kb-labs-cli/packages/core",
    "@kb-labs/cli-commands": "link:../../../kb-labs-cli/packages/commands",
    "commander": "^12.1.0",
    "colorette": "^2.0.20"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
