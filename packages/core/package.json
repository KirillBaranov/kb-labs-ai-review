{
                "name": "@sentinel/core",
                "version": "0.1.0",
                "type": "module",
                "main": "dist/index.js",
                "exports": {
                  ".": "./dist/index.js"
                },
                "bin": {
                  "sentinel-build-context": "dist/scripts/build-ai-context.js",
                  "sentinel-normalize": "dist/scripts/ai-review-normalize.js",
                  "sentinel-render": "dist/scripts/ai-review-render.js",
                  "sentinel-diff": "dist/scripts/ai-review-diff.js"
                },
                "files": [
                  "dist"
                ],
                "scripts": {
                  "build": "tsup src/index.ts src/scripts/* --format esm --dts --sourcemap",
                  "dev": "tsx src/scripts/build-ai-context.ts profiles/frontend dist/ai-review-context.md",
                  "type-check": "tsc -p tsconfig.json --noEmit",
                  "lint": "eslint .",
                  "test": "vitest run"
                },
                "dependencies": {
                  "yaml": "^2.5.1"
                },
                "devDependencies": {
                  "tsup": "^8.3.5",
                  "tsx": "^4.19.1",
                  "typescript": "^5.6.3",
                  "vitest": "^2.1.3"
                }
              }