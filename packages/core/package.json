{
  "name": "@sentinel/core",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --sourcemap",
    "dev": "ts-node src/index.ts",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.3.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "ts-node": "^10.9.2",
    "eslint": "^9.9.0"
  },
  "license": "MIT",
  "author": { "name": "Kirill Baranov", "url": "https://github.com/KirillBaranov" },
  "repository": { "type": "git", "url": "git+https://github.com/kirill-baranov/sentinel-ai.git" },
  "bugs": { "url": "https://github.com/kirill-baranov/sentinel-ai/issues" },
  "homepage": "https://github.com/kirill-baranov/sentinel-ai#readme",
  "keywords": ["ai", "code-review", "framework", "static-analysis", "sentinel"],
  "engines": { "node": ">=18.0.0" },
  "publishConfig": { "access": "public" }
}
