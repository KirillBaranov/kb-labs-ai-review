{
  "sync": {
    "enabled": true,
    "only": [
      "ci",
      "drift-check"
    ],
    "scope": "managed-only",
    "force": false,
    "disabled": [],
    "overrides": {},
    "targets": {
      "ci": {
        "from": ".github/workflow-templates",
        "to": ".github/workflows",
        "type": "dir"
      }
    }
  },
  "profile": "frontend",
  "profilesDir": "packages/profiles",
  "provider": "local",
  "failOn": "major",
  "maxComments": 50,
  "out": {
    "root": ".kb-labs",
    "contextDir": "context",
    "reviewsDir": "reviews",
    "analyticsDir": "analytics",
    "exportsDir": "exports"
  },
  "analytics": {
    "enabled": true,
    "salt": "changeme",
    "privacy": "team",
    "mode": "byRun"
  }
}
